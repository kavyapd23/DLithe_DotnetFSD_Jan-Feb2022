                <p id="errorMessage"></p>
               <p id="message"></p>
	
	<script>
		try
		{
            var a=10;
            
		}
		catch(ex)
		{
			document.getElementById("errorMessage").innerHTML = ex;
		}
        finally
        {
         document.getElementById("message").innerHTML = "finally block executed"+ a;
        }
        function calculate_age(dob){
            var diff_ms = Date.now - dob.getTime();
            var age_dt=new Date(diff_ms);
            return Math.abs(age_dt.getUTCFullYear()-1999);
            document.write(calcuate_age(new Date(2022,05,23)));
            document.write(calcuate_age(new Date(2022,03,29)));
        }
    </script>
