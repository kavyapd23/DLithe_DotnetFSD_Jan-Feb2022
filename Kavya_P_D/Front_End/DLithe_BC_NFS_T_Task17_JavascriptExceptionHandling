                 <h1>MATH Functions</h1>
                 <p id="demo"></p>
                 <script>
    
                    //"use strict"; 
                    a=10;
                 document.getElementById("demo").innerHTML = 
                 "<p><b>Math.E:</b> " + Math.E + "</p>" +
                 "<p><b>Math.PI:</b> " + Math.PI + "</p>" +
                 "<p><b>Math.SQRT2:</b> " + Math.SQRT2 + "</p>" +
                 "<p><b>Math.SQRT1_2:</b> " + Math.SQRT1_2 + "</p>" +
                 "<p><b>Math.LN2:</b> " + Math.LN2 + "</p>" +
                 "<p><b>Math.LN10:</b> " + Math.LN10 + "</p>" +
                 "<p><b>Math.LOG2E:</b> " + Math.LOG2E + "</p>" +
                 "<p><b>Math.Log10E:</b> " + Math.LOG10E + "</p>";
                 </script>
                 <hr>
                 <script>
                 document.write(Math.round(8.6)+"<br>");
                 document.write(Math.round(-8.6)+"<br>");
                 document.write(Math.ceil(8.6)+"<br>");
                 document.write(Math.ceil(7.2)+"<br>");
                 document.write(Math.floor(5.7)+"<br>");
                 document.write(Math.trunc(4.7)+"<br>");
                 document.write(Math.sign(0)+"<br>");
                 document.write(Math.sign(-3.7)+"<br>");
                 document.write(Math.pow(4,2)+"<br>");
                 document.write(Math.sqrt(81)+"<br>");
                 document.write(Math.abs(-1)+"<br>");
                 document.write(Math.sin(1)+"<br>");
                 document.write(Math.cos(1)+"<br>");
                 document.write(Math.tan(1)+"<br>");
                 document.write(Math.min(0, 150, 30, 20, -8, -9)+"<br>");
                 document.write(Math.max(0, 800, 130, 820, -8, -700)+"<br>");
                 document.write(Math.random()+"<br>");   
                 document.write(Math.log(1)+"<br>");  
                 document.write(Math.floor(Math.random() * 100)+"<br>");
                </script>
