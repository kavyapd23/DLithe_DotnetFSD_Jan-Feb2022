        <h1>Splice()</h1>
       <script>
            const weeks = ['Sunday', 'Tuesday', 'Thurday', ];
            //splice(start, deleteCount, item1, item2, itemN)
weeks.splice(1, 0, 'Monday');
// inserts at index 1
document.write(weeks);
document.writeln("<br/>"); 
weeks.splice(3, 1, 'Wednesay');
// replaces 1 element at index 4
document.write(weeks);
document.writeln("<br/>"); 

var a = ['one', 'two', 'three', 'four', 'five']
document.write(a);
document.writeln("<br/>"); 
var b = a.splice(1, 3);
document.write(b);
document.writeln("<br/>"); 
document.write(a);
document.writeln("<br/>"); 
a.splice(2, 0,'four');
document.write(a);
document.writeln("<br/>"); 
</script>
<hr>
<h1>concat()</h1>
<script>
const array1 = ['one ', 'two', 'three'];
const array2 = ['a', 'b', 'c'];
const array3 = array1.concat(array2);
document.write(array3);
 </script>
<hr>
 <script>
     const word=['hello'];
     const word2=['everyone'];
     const word3=word.concat(word2);
     document.write(word3);
 </script>
 <hr>
 <h1>entries method</h1>
 <script>
    var a = ['hi', 'hello', 'hey','hiii','heeee'];
var iterator = a.entries();

for (let hi  of iterator)
{
document.write(hi+"<br>");
}
</script>
<hr>
<h1>reverse()</h1>
<script>
    var a = [101, 201, 301];

document.write(a);

a.reverse();
document.writeln("<br/>")    ; 

document.write(a); 
</script>
<hr>
<h1> indexof() and lastindexof()</h1>
<script>
    var colors = ["orange", "red", "yellow", "purple","red","pink"];
    var a=colors.indexOf("red") ;  // Returns 2
    document.write(a);
    document.writeln("<br/>"); 
    a=colors.lastIndexOf("red"); 
    document.write(a);
        </script>
<hr>
<h1>slice()</h1>
<script>
     var colors = ["orange", "red", "yellow", "purple","red","pink"];
    
var a = colors.slice(-6, -3);
document.write(a);
document.writeln("<br/>"); 
var b = colors.slice(1, 4);
document.write(b);
</script>        
 <hr>
<h1>sort()</h1>
<script>
    var arr = [28, 52, 86, 14, 46]
document.write(arr.sort());
document.write(arr);
</script>
<h1>filter()</h1>
<script>
    var words = ['nisarga', 'kavita', 'Apoorva', 'kratika', 'swapna', 'anjana'];
    
    var result = words.filter(word => word.length == 6);
    
    document.write(result);
    </script>
<hr>
